    "version": "0.2.0",
        "configurations": [
        {
            "name": "Win32 mingw",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build_mingw32/onsyuri.exe",
            "args": ["--root", "test", "--font", "test/default.ttf", "--save-dir", "test"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/asset",
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "${env:MSYS2_HOME}/mingw32/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "cppdbg GDB launch (Windows) start!",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }, 
            ]
        },

